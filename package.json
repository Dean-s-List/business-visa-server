{
    "name": "business-visa-server",
    "version": "1.0.0",
    "description": "business-visa-server",
    "main": "src/index.ts",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "yarn nodemon",
        "build": "tsc --build",
        "lint": "yarn eslint --no-cache ./src/**/*.ts",
        "lint:fix": "yarn eslint --no-cache --fix ./src/**/*.ts",
        "prepare": "husky install",
        "prettier:check": "yarn prettier --check .",
        "prettier:write": "yarn prettier --write .",
        "db:introspect": "yarn drizzle-kit introspect:mysql",
        "db:push": "yarn drizzle-kit push:mysql",
        "db:studio": "yarn drizzle-kit studio",
        "db:generate": "yarn drizzle-kit generate:mysql",
        "db:drop": "yarn drizzle-kit drop",
        "scripts:update-earnings": "yarn ts-node --esm ./src/scripts/updateBusinessVisaHoldersEarnings.ts",
        "scripts:update-expiry": "yarn ts-node --esm ./src/scripts/updateBusinessVisaHolderExpiryDate.ts"
    },
    "dependencies": {
        "@planetscale/database": "^1.10.0",
        "@solana/web3.js": "^1.78.1",
        "@upstash/qstash": "^0.3.6",
        "airtable": "^0.12.2",
        "axios": "^1.4.0",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "dotenv": "^16.3.1",
        "drizzle-orm": "^0.27.2",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.0",
        "helmet": "^7.0.0",
        "http-status": "^1.6.2",
        "imagekit": "^4.1.3",
        "isomorphic-fetch": "^3.0.0",
        "mysql2": "^3.9.3",
        "node-cron": "^3.0.2",
        "nodemon": "^3.0.1",
        "resend": "^0.17.2",
        "ts-node": "^10.9.1",
        "undici": "^5.22.1",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.7",
        "@commitlint/config-conventional": "^17.6.7",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.1",
        "@types/node-cron": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "drizzle-kit": "^0.19.11",
        "esbuild-register": "^3.4.2",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^36.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "husky": "^8.0.3",
        "lint-staged": ">=10",
        "prettier": "3.0.0",
        "typescript": "^5.1.6",
        "zod": "^3.21.4"
    },
    "lint-staged": {
        "*.{ts,js,json}": [
            "prettier --write"
        ]
    }
}
